{
    "name": "my-monorepo",
    "private": true,
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev:be": "cd apps/backend && pnpm run start:dev",
        "dev:fe": "cd apps/frontend && pnpm run dev",
        "dev": "concurrently --names \"SERVER,CLIENT\" -c \"bgMagenta.bold,bgGreen.bold\" \"pnpm dev:be\" \"pnpm dev:fe\"",
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "turbo run build",
        "lint": "turbo run lint",
        "clean": "npx rimraf node_modules apps/frontend/node_modules apps/backend/node_modules packages/types/node_modules pnpm-lock.yaml",
        "reinstall": "pnpm run clean && pnpm install",
        "reset": "pnpm run clean && pnpm store prune && pnpm install"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "packageManager": "pnpm@10.14.0",
    "devDependencies": {
        "@types/node": "^24.5.1",
        "@typescript-eslint/eslint-plugin": "^8.44.0",
        "@typescript-eslint/parser": "^8.44.0",
        "concurrently": "^9.2.1",
        "eslint": "^9.35.0",
        "eslint-config-next": "15.5.3",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "prettier": "^3.6.2",
        "rimraf": "^5.0.10",
        "ts-node": "^10.9.2",
        "turbo": "^2.5.6",
        "typescript": "^5.9.2"
    },
    "dependencies": {
        "@nestjs/config": "^4.0.2",
        "@nestjs/typeorm": "^11.0.0",
        "pg": "^8.16.3",
        "typeorm": "^0.3.26"
    }
}
